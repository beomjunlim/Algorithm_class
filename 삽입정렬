#include <iostream>
using namespace std;

void Insertion_Sort(int arr[], int n)
{
  int i, j;
  for (i = 1; i < n; i++)
  {
    int key = arr[i];
    for (j = i - 1; j >= 0 && arr[j] > key; j--)
    {
      arr[j + 1] = arr[j];
    }
    arr[j + 1] = key;
  }
}
int main()
{
  int arr[10] = {4, 5, 9, 6, 8, 10, 1, 3, 2, 7};
  Insertion_Sort(arr, 10);

  for (int i = 0; i < 10; i++)
  {
    cout << arr[i] << " ";
  }
  cout << endl;
}
